{ 
    "$schema" : "https://json.schemastore.org/tsconfig",
    "_version" : " 20.1.0 ",

   "compilerOptions" : {
      "lib": [ "ES2023" ],
      "module": "commonjs",
      "target": "ES2023",
      "strict": true,
      "esModuleInterop": true,
      "skipDefaultLibCheck": false,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "node",
      "outDir": "./dist",
      "sourceMap": true,
      "declaration": true,
      "typeRoots": ["./node_modules/@types", "./src/types"]
   },
  "include": [
    "src/**/*",
    "src/types/**/*"
  ]
}